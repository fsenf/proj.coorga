#!/usr/bin/env python

import sys
import coorga.object_creation.save_cluster_properties.main as main


######################################################################
######################################################################



if __name__ == '__main__':


    # get input ------------------------------------------------------
    fname = sys.argv[1]
        

    # set the segmentation parameters by experient name
    try:
        expname = sys.argv[2]
    except:
        expname = 'basic'

    # set the variable name
    try:
        varname = sys.argv[3]
    except:
        varname = 'bt108'
        
    # ================================================================


    main( fname, expname, varname )
